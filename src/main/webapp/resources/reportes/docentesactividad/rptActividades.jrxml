	<queryString>
		<![CDATA[SELECT a.id_adicional, a.id_distributivo, a.id_tipo_actividad, c.actividad, a.observaciones, 
a.horas, b.id_periodo, b.id_docente
FROM adicionales a, distributivo_cabecera b, tipo_actividad c
where a.id_distributivo = b.id_distributivo
and c.id_tipo_actividad = a.id_tipo_actividad
and a.estado='ACTIVO' AND b.estado='ACTIVO' and c.estado='ACTIVO'
and b.id_distributivo = $P{ID_CABECERA}

union all

select null as id_adicional, null as id_distributivo, null as id_tipo_actividad, null as actividad, 
null as observaciones, 
null as horas, null as d_periodo, null as id_docente
where (SELECT count(*)
FROM adicionales a, distributivo_cabecera b, tipo_actividad c
where a.id_distributivo = b.id_distributivo
and c.id_tipo_actividad = a.id_tipo_actividad
and a.estado='ACTIVO' AND b.estado='ACTIVO' and c.estado='ACTIVO'
and b.id_distributivo = $P{ID_CABECERA}) = 0

Order by id_adicional asc]]>
