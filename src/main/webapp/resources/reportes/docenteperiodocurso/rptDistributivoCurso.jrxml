	<queryString>
		<![CDATA[select cab.id_cabecera_distributivo,cab.id_periodo,cab.id_docente,cab.cedula,cab.codigo_senescyt,cab.nombre_docente,
cab.titularidad,cab.dedicacion,det.id_detalle_distributivo,det.paralelo,det.curso,det.materia,det.horas, det.id_curso,
(select case when sum(horas) is null then 0 else sum(horas) end as horas_adicionales
from adicionales
where estado = 'ACTIVO' and id_distributivo = cab.id_cabecera_distributivo) as horas_adicionales
from (select d.id_distributivo as id_cabecera_distributivo,d.id_periodo,doc.id as id_docente,doc.cedula,doc.codigo_senescyt,
doc.nombre as nombre_docente,t.nombre as titularidad,ded.nombre as dedicacion
from distributivo_cabecera d, docente doc, titularidad t, dedicacion ded
where d.estado = 'ACTIVO' and doc.estado = 'ACTIVO' and t.estado = 'ACTIVO'
and ded.estado = 'ACTIVO' and d.id_docente = doc.id and doc.id_titularidad = t.id_titularidad
and d.id_dedicacion = ded.id) cab,
(select d.id_detalle as id_detalle_distributivo, d.id_distributivo as id_cabecera_distributivo, p.nombre as paralelo,
c.nombre as curso, m.nombre as materia,md.num_horas as horas, 	C.id_curso
from distributivo_detalle d, paralelo p, curso c, materia m, malla_detalle md
where d.estado = 'A' and p.estado = 'ACTIVO' and c.estado = 'ACTIVO' and m.estado = 'ACTIVO'
and md.estado = 'ACTIVO' and d.id_paralelo = p.id_paralelo and d.id_curso = c.id_curso
and d.id_materia = m.id and d.id_malla_detalle = md.id_detalle) det
where cab.id_cabecera_distributivo = det.id_cabecera_distributivo and cab.id_periodo = $P{ID_PERIODO}
and det.id_curso= $P{ID_CURSO}
Order by cab.nombre_docente asc]]>
	</queryString>
