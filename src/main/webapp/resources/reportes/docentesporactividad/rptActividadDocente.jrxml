	<queryString>
		<![CDATA[select doc.cedula as cedula_docente,doc.nombre as docente,t.nombre as titularidad, ded.nombre as dedicacion,
doc.codigo_senescyt, ta.actividad, a.horas, ta.id_tipo_actividad
from distributivo_cabecera d, adicionales a, tipo_actividad ta,docente doc, titularidad t, dedicacion ded
where d.estado = 'ACTIVO' and a.estado = 'ACTIVO' and d.estado = 'ACTIVO'
and t.estado = 'ACTIVO' and ded.estado = 'ACTIVO' and d.id_dedicacion = ded.id and 
d.id_docente = doc.id and doc.id_titularidad = t.id_titularidad and d.id_distributivo = a.id_distributivo
and ta.id_tipo_actividad = a.id_tipo_actividad and d.id_periodo = $P{ID_PERIODO}

union all

select null as cedula_docente,null as docente,null as titularidad, null as dedicacion,
null as codigo_senescyt, null as actividad, null as horas, null as id_tipo_actividad
where  (select count(*)
from distributivo_cabecera d, adicionales a, tipo_actividad ta,docente doc, titularidad t, dedicacion ded
where d.estado = 'ACTIVO' and a.estado = 'ACTIVO' and d.estado = 'ACTIVO'
and t.estado = 'ACTIVO' and ded.estado = 'ACTIVO' and d.id_dedicacion = ded.id and 
d.id_docente = doc.id and doc.id_titularidad = t.id_titularidad and d.id_distributivo = a.id_distributivo
and ta.id_tipo_actividad = a.id_tipo_actividad and d.id_periodo = $P{ID_PERIODO}) = 0]]>
	</queryString>
